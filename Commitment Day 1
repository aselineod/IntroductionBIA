# Create two 2x3 matrices.
matrix1 <- matrix(c(1, 2, 3, 4, 5, 6), nrow = 3)
matrix2 <- matrix(c(7, 8, 9, 10, 11, 12), nrow = 3)

# Print
print("The first matrix is:")
print(matrix1)
print(The second matrix is:")
print(matrix)

# Add the matrices
result1 <- matrix1 + matrix2

# Substract the matrices
result2 <- matrix1 - matrix2

# Multply the matrices
result3 <- matrix1 * matrix2

# Divide the matrices
result4 <- matrix1 / matrix2

# Print results
print("The results of Addition of is:")
print(result1)
print("The results of Substraction of is:")
print(result2)
print("The results of Multiplication of is:")
print(result3)
print("The results of Division of is:")
print(result4)
